{
  "name": "costs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:client": "nuxt-ts --port 4000",
    "dev:server": "ts-node-dev --transpile-only --respawn -r tsconfig-paths/register server/index.ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "heroku-postbuild": "npm run build-server",
    "start-server": "node -r tsconfig-paths/register dist/server/index.js",
    "build-server": "tsc -p tsconfig-build.json",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {

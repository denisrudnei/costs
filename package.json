    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/toast": "^3.3.1",
    "apexcharts": "^3.22.0",
    "apollo-client": "^2.6.10",
    "apollo-server-express": "^2.18.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.20.0",
    "class-validator": "^0.12.2",
    "consola": "^2.15.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dinero.js": "^1.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.13",
    "nuxt": "^2.14.6",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.1.0",
